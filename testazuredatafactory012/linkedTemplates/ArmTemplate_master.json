{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "testazuredatafactory012"
		},
		"LS_Blob_storage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Blob_storage1'"
		},
		"LS_SQLConnection_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SQLConnection'"
		},
		"target_AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'target_AzureDataLakeStorage1'"
		},
		"LS_HTTPConnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().baseURL}"
		},
		"LS_SQLConnection_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqldatabase0.database.windows.net"
		},
		"LS_SQLConnection_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldatabase1"
		},
		"LS_SQLConnection_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "hero"
		},
		"target_AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakegen202345.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "testazuredatafactory012_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_Blob_storage1_connectionString": {
						"value": "[parameters('LS_Blob_storage1_connectionString')]"
					},
					"LS_SQLConnection_password": {
						"value": "[parameters('LS_SQLConnection_password')]"
					},
					"target_AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('target_AzureDataLakeStorage1_accountKey')]"
					},
					"LS_HTTPConnection_properties_typeProperties_url": {
						"value": "[parameters('LS_HTTPConnection_properties_typeProperties_url')]"
					},
					"LS_SQLConnection_properties_typeProperties_server": {
						"value": "[parameters('LS_SQLConnection_properties_typeProperties_server')]"
					},
					"LS_SQLConnection_properties_typeProperties_database": {
						"value": "[parameters('LS_SQLConnection_properties_typeProperties_database')]"
					},
					"LS_SQLConnection_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQLConnection_properties_typeProperties_userName')]"
					},
					"target_AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('target_AzureDataLakeStorage1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "testazuredatafactory012_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'testazuredatafactory012_ArmTemplate_0')]"
			]
		}
	]
}